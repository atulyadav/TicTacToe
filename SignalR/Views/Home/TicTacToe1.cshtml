@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>TicTacToe1</title>
    <script src="~/Scripts/jquery-2.1.3.min.js"></script>
    @*    <script src="~/Scripts/jquery-migrate-1.2.1.min.js"></script>*@
    <script src="~/Scripts/jquery-ui-1.8.24.min.js"></script>
    <link href="~/Content/bootstrap/css/bootstrap-theme.min.css" rel="stylesheet" />
    <link href="~/Content/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <script src="~/Content/bootstrap/js/bootstrap.min.js"></script>
    @*<script src="~/Content/bootstrap/js/bootstrap-modal.js"></script>*@
    @*<script src="~/Content/bootstrap/js/npm.js"></script>*@
    @*   <script src="@Url.Content("~/Scripts/jquery-1.8.2.min.js")"></script>
    <!--Reference the SignalR library. -->
    <script src="@Url.Content("~/Scripts/jquery.signalR-1.0.0.js")"></script>
    <!--Reference the autogenerated SignalR hub script. -->
    <script src="@Url.Content("~/signalr/hubs")"></script>*@
    <link href="@Url.Content("~/Content/css/TicTacToe.css")" rel="stylesheet" type="text/css" />
    @*<script src="@Url.Content("~/Scripts/TicTacToe/TicTacToe.js")"></script>*@
    <script src="@Url.Content("~/Scripts/TicTacToe/TicTacToeSignalR.js")"></script>
</head>
<body>
    <div>
        <div class="alert alert-success" id="connection-result" data-es="Aquí se muestra el resultado del evento">
            Connection Status...
        </div>
        <div class="row">
            <div class="col-md-2">
                <button type="button" class="btn btn-primary btn-lg btn-block">Block level button</button>
                <button type="button" class="btn btn-default btn-lg btn-block">Block level button</button>
            </div>
            <div class="col-md-5">.col-md-4</div>
            <div class="col-md-5">
                <div class="alert alert-success" id="events-result" data-es="Aquí se muestra el resultado del evento">
                    Here is the result of event.
                </div>
                <table class="table hover condensed striped" data-cache="false" data-toggle="table">
                    <thead>
                        <tr>
                            <th data-field="connectionId">Connection ID</th>
                            <th data-field="userName">User Name</th>
                            <th data-field="status">Status</th>
                            <th data-field="connectToUser">...</th>
                        </tr>
                    </thead>
                </table>
            </div>
        </div>
    </div>
    <div class="modal fade" id="userNameDialog" role="dialog">
        <div class="modal-dialog">
            <div class="modal-content">
                <div class="modal-header">Please Enter your name </div>
                <div class="modal-body">
                    <div class="row">
                        <input type="text" class="form-control" />
                    </div>
                </div>
                <div class="modal-footer">
                    <a href="#" data-dismiss="modal" class="btn">Close</a>
                    <a href="#" class="btn btn-primary" id="btn-save-user">Save</a>
                </div>
            </div>
        </div>
    </div>
</body>
</html>
